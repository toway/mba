

    <label>{{field.widget.url_prefix}}</label>

    
    <input  type="text" class="" name="{{field.name}}" value="{{cstruct}}"
    {% if field.widget.size %}
     size="{{field.widget.size}}"
    {% endif %}
    {% if field.widget.css_class %}
        class="{{field.widget.css_class}} textInput"
    {% else %}
        class="textInput"
    {% endif %}
     id="{{field.oid}}"/>


{% if field.widget.mask %}
<script type="text/javascript">
  deform.addCallback(
     '{{field.oid}}',
     function (oid) {
        $("#" + oid).mask("{{field.widget.mask}}",
                          {placeholder:"{{field.widget.mask_placeholder}}"});
     });
</script>
{% endif %}




<input name="{{field.name}}" id="{{field.oid}}" type="text" value="{{cstruct|join(', ')}}" />
<script type="text/javascript"
  >
  deform.addCallback(
    '{{field.oid}}',
    function(oid) {
      $('#' + oid).tagit({
          availableTags: {{field.widget.available_tags}},
          allowSpaces: true
      });
    }
  )
</script>
